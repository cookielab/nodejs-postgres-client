{
  "name": "@cookielab.io/postgres-client",
  "version": "3.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cookielab/nodejs-postgres-client.git"
  },
  "author": {
    "name": "Pavel Kucera",
    "url": "https://github.com/pavelkucera/"
  },
  "contributors": [
    {
      "name": "Martin Štekl",
      "url": "https://github.com/stekycz"
    },
    {
      "name": "Patrik Votoček",
      "url": "https://github.com/Vrtak-CZ"
    },
    {
      "name": "Tomáš Markacz",
      "url": "https://github.com/markatom"
    }
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "bin/build",
    "watch": "bin/watch",
    "lint": "bin/lint || true",
    "typescript": "bin/typescript || true",
    "test": "bin/jest",
    "check-all": "bin/check-all || true",
    "prepare": "bin/build"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "pg-async": "^3.1.0",
    "pg-query-stream": "^2.0.0",
    "semaphore-async-await": "^1.5.1"
  },
  "peerDependencies": {
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "@cookielab.io/eslint-config-server": "0.6.0",
    "@cookielab.io/stream-async-wrappers": "0.3.0",
    "@types/jest": "24.0.17",
    "@types/lodash.snakecase": "4.1.6",
    "@types/node": "12.7.1",
    "@types/pg": "7.4.14",
    "@types/pg-query-stream": "1.0.2",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "5.16.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-sort-imports-es6-autofix": "0.4.0",
    "flowgen": "1.9.2",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "lodash.snakecase": "4.1.1",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.(ts|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
